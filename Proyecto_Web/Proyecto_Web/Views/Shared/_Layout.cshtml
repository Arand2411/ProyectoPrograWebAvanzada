<!DOCTYPE html>
<html lang="zxx">
<head>
    <!-- Meta Tag -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name='copyright' content=''>
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Title Tag  -->
    <title>Eshop - eCommerce HTML5 Template.</title>
    <!-- Favicon -->
    <link rel="icon" type="image/png" href="~/page/images/favicon.png">
    <!-- Web Font -->
    <link href="https://fonts.googleapis.com/css?family=Poppins:200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&display=swap" rel="stylesheet">
    <!-- StyleSheet -->
    <link rel="stylesheet" href="https://cdn.datatables.net/2.0.8/css/dataTables.bootstrap4.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.css">
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/page/css/bootstrap.css">
    <!-- Magnific Popup -->
    <link rel="stylesheet" href="~/page/css/magnific-popup.min.css">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/page/css/font-awesome.css">
    <!-- Fancybox -->
    <link rel="stylesheet" href="~/page/css/jquery.fancybox.min.css">
    <!-- Themify Icons -->
    <link rel="stylesheet" href="~/page/css/themify-icons.css">
    <!-- Nice Select CSS -->
    <link rel="stylesheet" href="~/page/css/niceselect.css">
    <!-- Animate CSS -->
    <link rel="stylesheet" href="~/page/css/animate.css">
    <!-- Flex Slider CSS -->
    <link rel="stylesheet" href="~/page/css/flex-slider.min.css">
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="~/page/css/owl-carousel.css">
    <!-- Slicknav -->
    <link rel="stylesheet" href="~/page/css/slicknav.min.css">

    <!-- Eshop StyleSheet -->
    <link rel="stylesheet" href="~/page/css/reset.css">
    <link rel="stylesheet" href="~/page/css/styleIndex.css">
    <link rel="stylesheet" href="~/page/css/responsive.css">

    <style>
        .right-bar {
            display: flex;
            justify-content: flex-end; /* Alínea el contenido a la derecha */
            align-items: center; /* Centra verticalmente los elementos */
        }

        .icon-bar {
            display: flex;
            align-items: center;
        }

            .icon-bar .single-bar {
                margin-left: 20px; /* Espaciado entre íconos */
            }
    </style>
</head>
<body class="js">
    <!-- Preloader -->
    <div class="preloader">
        <div class="preloader-inner">
            <div class="preloader-icon">
                <span></span>
                <span></span>
            </div>
        </div>
    </div>
    <!-- End Preloader -->
    <!-- Header -->
    <header class="header shop">
        <!-- Topbar -->
        <div class="topbar">
            <div class="container">
                <div class="row">
                    <div class="col-lg-4 col-md-12 col-12">
                    </div>
                    <div class="col-lg-8 col-md-12 col-12">
                        <!-- Top Right -->
                        <!-- End Top Right -->
                    </div>
                </div>
            </div>
        </div>
        <!-- End Topbar -->
        <div class="middle-inner">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-lg-2 col-md-2 col-12">
                        <!-- Logo and Search Form -->
                        <div class="logo-search">
                            <!-- Logo -->
                            <div class="logo">
                                <a href="index.html"><img src="~/page/images/logo.png" alt="logo"></a>
                            </div>
                            <!-- Search Form -->
                            <div class="search-bar-top">
                                <form class="search-form">
                                    <input name="search" placeholder="Search Products Here....." type="search">
                                    <button class="btnn"><i class="ti-search"></i></button>
                                </form>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-10 col-md-10 col-12">
                        <div class="right-bar d-flex justify-content-end">
                            <!-- Icons and Right Content -->
                            <div class="icon-bar d-flex align-items-center">
                                <div class="single-bar">
                                    <a href="#" class="single-icon"><i class="fa fa-heart-o" aria-hidden="true"></i></a>
                                </div>
                                <div class="single-bar">
                                    <a href="#" class="single-icon"><i class="fa fa-user-circle-o" aria-hidden="true"></i></a>
                                </div>
                                <div class="single-bar shopping">
                                    <a href="#" class="single-icon"><i class="ti-bag"></i> <span class="total-count">2</span></a>
                                </div>
                                <div class="single-bar">
                                    <div class="right-content">
                                        <ul class="list-main">
                                            <li><i class="ti-user"></i> <a href="#">My account</a></li>
                                            <li><i class="ti-power-off"></i><a href="@Url.Action("LoginUsuario", "Usuario")">Login</a></li>
                                            <li><i class="ti-pencil"></i><a href="@Url.Action("RegistrarProducto", "Producto")">Producto</a></li>
                                            <li><i class="ti-book"></i><a href="@Url.Action("ConsultarProductos", "Producto")">Producto</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Header Inner -->
        <div class="header-inner">
            <div class="container">
                <div class="cat-nav-head">
                    <div class="row">
                        <div class="col-lg-3">
                            <div class="all-category">
                                <h3 class="cat-heading"><i class="fa fa-bars" aria-hidden="true"></i>CATEGORIES</h3>
                            </div>
                        </div>
                        <div class="col-lg-9 col-12">
                            <div class="menu-area">
                                <!-- Main Menu -->
                                <nav class="navbar navbar-expand-lg">
                                    <div class="navbar-collapse">
                                        <div class="nav-inner">
                                            <ul class="nav main-menu menu navbar-nav">
                                                <li class="active"><a href="#">Home</a></li>
                                                <li><a href="#">Product</a></li>
                                                <li><a href="contact.html">Contact Us</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </nav>
                                <!--/ End Main Menu -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--/ End Header Inner -->
    </header>

    <!-- /End Cowndown Area -->
    <!-- Start Shop Blog  -->
    @RenderBody()
    <!-- Start Footer Area -->
    <footer class="footer">
        <!-- Footer Top -->
        <div class="footer-top section">
            <div class="container">
                <div class="row">
                    <div class="col-lg-5 col-md-6 col-12">
                        <!-- Single Widget -->
                        <div class="single-footer about">
                            <div class="logo">
                                <a href="index.html"><img src="~/page/images/logo2.png" alt="#"></a>
                            </div>
                        </div>
                        <!-- End Single Widget -->
                    </div>
                    <div class="col-lg-2 col-md-6 col-12">
                        <!-- Single Widget -->
                        <div class="single-footer links">
                            <h4>Information</h4>
                            <ul>
                                <li><a href="#">About Us</a></li>
                                <li><a href="#">Faq</a></li>
                                <li><a href="#">Terms & Conditions</a></li>
                                <li><a href="#">Contact Us</a></li>
                                <li><a href="#">Help</a></li>
                            </ul>
                        </div>
                        <!-- End Single Widget -->
                    </div>
                    <div class="col-lg-2 col-md-6 col-12">
                        <!-- Single Widget -->
                        <div class="single-footer links">
                            <h4>Customer Service</h4>
                            <ul>
                                <li><a href="#">Payment Methods</a></li>
                                <li><a href="#">Money-back</a></li>
                                <li><a href="#">Returns</a></li>
                                <li><a href="#">Shipping</a></li>
                                <li><a href="#">Privacy Policy</a></li>
                            </ul>
                        </div>
                        <!-- End Single Widget -->
                    </div>
                    <div class="col-lg-3 col-md-6 col-12">
                        <!-- Single Widget -->
                        <div class="single-footer social">
                            <h4>Get In Tuch</h4>
                            <!-- Single Widget -->
                            <div class="contact">
                                <ul>
                                    <li>Shop Address</li>
                                    <li>Shop Address</li>
                                    <li>Shop Address</li>
                                    <li>info@eshop.com</li>
                                    <li>+1 800 123 1234</li>
                                </ul>
                            </div>
                            <!-- End Single Widget -->
                            <ul>
                                <li><a href="#"><i class="ti-facebook"></i></a></li>
                                <li><a href="#"><i class="ti-twitter"></i></a></li>
                                <li><a href="#"><i class="ti-flickr"></i></a></li>
                                <li><a href="#"><i class="ti-instagram"></i></a></li>
                            </ul>
                        </div>
                        <!-- End Single Widget -->
                    </div>
                </div>
            </div>
        </div>
        <!-- End Footer Top -->
        <div class="copyright">
            <div class="container">
                <div class="inner">
                    <div class="row">
                        <div class="col-lg-6 col-12">
                            <div class="left">
                                <p>Copyright © 2020 <a href="http://www.wpthemesgrid.com" target="_blank">Wpthemesgrid</a>  -  All Rights Reserved.</p>
                            </div>
                        </div>
                        <div class="col-lg-6 col-12">
                            <div class="right">
                                <img src="~/page/images/payments.png" alt="#">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- /End Footer Area -->
    <!-- Jquery -->
    <script src="~/page/js/jquery.min.js"></script>
    <script src="~/page/js/jquery-migrate-3.0.0.js"></script>
    <script src="~/page/js/jquery-ui.min.js"></script>
    <!-- Popper JS -->
    <script src="~/page/js/popper.min.js"></script>
    <!-- Bootstrap JS -->
    <script src="~/page/js/bootstrap.min.js"></script>
    <!-- Color JS -->
    <script src="~/page/js/colors.js"></script>
    <!-- Slicknav JS -->
    <script src="~/page/js/slicknav.min.js"></script>
    <!-- Owl Carousel JS -->
    <script src="~/page/js/owl-carousel.js"></script>
    <!-- Magnific Popup JS -->
    <script src="~/page/js/magnific-popup.js"></script>
    <!-- Fancybox JS -->
    <script src="~/page/js/facnybox.min.js"></script>
    <!-- Waypoints JS -->
    <script src="~/page/js/waypoints.min.js"></script>
    <!-- Countdown JS -->
    <script src="~/page/js/finalcountdown.min.js"></script>
    <!-- Nice Select JS -->
    <script src="~/page/js/nicesellect.js"></script>
    <!-- Ytplayer JS -->
    <script src="~/page/js/ytplayer.min.js"></script>
    <!-- Flex Slider JS -->
    <script src="~/page/js/flex-slider.js"></script>
    <!-- ScrollUp JS -->
    <script src="~/page/js/scrollup.js"></script>
    <!-- Onepage Nav JS -->
    <script src="~/page/js/onepage-nav.min.js"></script>
    <!-- Easing JS -->
    <script src="~/page/js/easing.js"></script>
    <!-- Active JS -->
    <script src="~/page/js/active.js"></script>
    <!-- DATATABLE JS -->
    <script src="https://cdn.datatables.net/2.0.8/js/dataTables.js"></script>
    <script src="https://cdn.datatables.net/2.0.8/js/dataTables.bootstrap4.js"></script>
</body>
</html>
